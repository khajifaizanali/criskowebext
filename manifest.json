{
  "manifest_version": 3,
  "name": "Job Application Filler",
  "version": "2.0",
  "description": "Automates job application form filling",
  "permissions": ["activeTab", "storage","http://localhost:8000/criskoweb/parse_html"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
